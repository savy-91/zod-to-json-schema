{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2017",
    "declaration": true,
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist", "test/**/*", "src/lab.ts", "deno_dist/",
    "src/**/*.deno.ts",
    "src/**/*.deno.tsx"
  ]
}
